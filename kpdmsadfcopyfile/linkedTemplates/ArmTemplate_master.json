{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "kpdmsadfcopyfile"
		},
		"ls_blobstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blobstorage'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kpdmskeyvault.vault.azure.net/"
		},
		"AzureKeyVaultdest_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kpdmskeyvault.vault.azure.net/"
		},
		"AzureSqlDatabase1_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "sqlconn1"
		},
		"SqlLinkedService_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kpdmskeyvault.vault.azure.net/"
		},
		"ls_destistorage_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "destkeyvault"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "kpdmsadfcopyfile_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_blobstorage_connectionString": {
						"value": "[parameters('ls_blobstorage_connectionString')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"AzureKeyVaultdest_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVaultdest_properties_typeProperties_baseUrl')]"
					},
					"SqlLinkedService_properties_typeProperties_baseUrl": {
						"value": "[parameters('SqlLinkedService_properties_typeProperties_baseUrl')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_connectionString_secretName')]"
					},
					"ls_destistorage_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_destistorage_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}